                        {
                            "isCondition": "True",
                            "Type": "Offset",
                            "Units": "offset",
                            "Description": "Offset from the Y end point to the second control point"
                        },
                    "R":
                        {
                            "isCondition": "False"
                        },
                    "S":
                        {
                            "isImplemented": "True",
